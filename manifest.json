{
    "manifest_version": 2,
    "name": "语雀笔记导出器",
    "version": "1.0",
    "description": "从语雀获取并保存笔记，支持标签筛选",
    "permissions": [
      "https://www.yuque.com/*",
      "cookies",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }